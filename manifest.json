{
  "manifest_version": 2,

  "name": "Webcrawler",
  "description": "This extension crawls the web starting from your current web page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Crawl!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
